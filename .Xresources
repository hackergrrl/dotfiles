! System-wide (for this user) re-colouring.
! Make dark/light blue more readable.
!*color4: #204a87
!*color12: #3465a4

Xcursor*theme: Adwaita

! Emacs
!emacs*font: -outline-Bitstream Vera Sans Mono-normal-r-normal-normal-12-90-96-96-c-*-iso8859-1

! Gvim
vim*font: -outline-Bitstream Vera Sans Mono-normal-r-normal-normal-12-90-96-96-c-*-iso8859-1

! Font settings
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

! urxvt
!urxvt*font: xft:monaco:size=10
!urxvt*font: -misc-fixed-medium-r-normal--13-120-75-75-C-70-iso10646-1
!URxvt.font:xft:droid sans mono:size=10
!URxvt.boldFont:xft:droid sans mono:size=10
!URxvt.font:xft:Terminus\ (TTF):size=12
!URxvt.letterSpace: -1
!URXvt.lineSpace: 1
urxvt*geometry: 90x32
!urxvt*foreground: gray
!urxvt*background: black
urxvt*scrollBar: off
urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0000/eeee
!urxvt*background: rgba:0000/0000/0000/ffff
!urxvt*transparent: true
!urxvt*shading: 15
urxvt*cursorColor: #5fd7ff
!urxvt*shading: 10
!urxvt*blurRadius: 3
urxvt.perl-ext-common: clipboard,url-select
urxvt.keysym.M-c:   perl:clipboard:copy
urxvt.keysym.M-v:   perl:clipboard:paste
urxvt.keysym.M-C-v: perl:clipboard:paste_escaped
urxvt.keysym.M-u:   perl:url-select:select_next
urxvt.url-select.launcher: chromium
urxvt.url-select.underline: true
!#include "/home/jason/.colours/dark"

! xlock
xlock.description: off
xlock.echokeys: on
xlock.echokey: *
xlock.showfps: on
xlock.info:
xlock.background: black
xlock.foreground: white
xlock.mode: space

!urxvt*font:             xft:terminus:pixelsize=10,xft:Inconsolata\ for\ Powerline:pixelsize=12
!urxvt*font:             xft:Ubuntu Mono:pixelsize=16:antialias=true:hinting=true:autohint=true
!urxvt*boldFont:         xft:terminus:pixelsize=8
!URxvt*font: xft:Terminus (TTF) for Powerline:pixelsize=12:antialias=false:hinting=false:autohint=false
